//배열 원소의 길이

#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

// 파라미터로 주어지는 문자열은 const로 주어집니다. 변경하려면 문자열을 복사해서 사용하세요.
int solution(const char* s) {
    int answer = 0, last_num = 0;
    char* ss = strdup(s);
    char* sss = strtok(ss, " ");
    
    while(sss != NULL){
        if(strcmp(sss, "Z") != 0){
            last_num = atoi(sss);
            answer += atoi(sss);  
        } 
        else answer -= last_num;
        
        sss = strtok(NULL, " ");
    }
    
    
    return answer;
}